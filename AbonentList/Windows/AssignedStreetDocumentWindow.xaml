<Window x:Class="AbonentList.Windows.AssignedStreetDocumentWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AbonentList.Windows"
        xmlns:localcontrols="clr-namespace:AbonentList.Controls"
        mc:Ignorable="d"
        Title="{Binding Path=WindowText, Mode=OneTime}" Height="1000" Width="860" WindowStartupLocation="CenterOwner">
    <Grid>
        <localcontrols:AssignedStreetDocumentControl DataContext="{Binding Path=DocumentControlModel, Mode=OneTime}" />
    </Grid>
</Window>
