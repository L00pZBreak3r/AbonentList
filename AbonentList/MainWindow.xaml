<Window x:Class="AbonentList.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AbonentList"
        xmlns:localcontrols="clr-namespace:AbonentList.Controls"
        mc:Ignorable="d"
        Title="{Binding Path=WindowText, Mode=OneTime}" Height="300" Width="600" ResizeMode="NoResize">
    <Grid>
    <localcontrols:MainDocumentControl DataContext="{Binding Path=DocumentControlModel, Mode=OneTime}" />
  </Grid>
</Window>
